farenheit=int(input("enter the value in farenheit"))
celsius=((farenheit-32)*5)/9
print("tempreture in celsius is",celsius)


celsius=int(input("enter the value in celsius"))
farenheit=(celsius*9/5)+32
print("tempreture in celsius is",farenheit)
  

